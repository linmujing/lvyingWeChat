/* pages/personCenter/myOrder/index.wxss */
.order_list{
  position: fixed;
  top:91rpx;
  left:0;
  width:100%;
}

/* 商品弹框列表 */
.coupon-box{
  position: fixed;
  width:100%;
  height:100%;
  bottom:-100%;
  z-index: 1000;
  background: rgba(0,0,0,0.5);
  transition: 0.5s;
}
.coupon-box-active{
  bottom: 0%;
}
.coupon-box-close{
  position: absolute;
  bottom: 0;
  text-align: center;
  width:100%;
  height: 90rpx;
  line-height: 90rpx;
  background: #fff;
  color:red;
  border: 1px solid #ccc;
}
.coupon-list{
  position: absolute;
  bottom: 90rpx;
  height: 80%;
  overflow: auto;
  width:100%;
  background: #f4f4f4;
}

